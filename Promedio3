//Programa para saber la edad promedio a la que se fraduan los estudiantes de la licenciatura en Fisica
//Autor:Maria Fernanda Hernandez Rodriguez
//Creado:30 de octubre de 2017

#include <vector>
#include <iostream>
using namespace std;

int main() {
        size_t talla =14;
        vector<int>  edad(talla);
        int suma=0;

        for (int i=0; i<talla; i++)
{
        cout<<("¿a que edad terminaste la carrera?")<<endl;
        cin >>edad[i];
        suma+=edad[i];
}

for (int j=0; j<talla;j++)
        cout<<edad[j]<<",";
        cout<<"la edad promedio de graduaciòn esperado es"<<suma/talla<<endl;

return 0;
            }
